//* to find sum of main diagonal elements of a matrix
#include <stdio.h>
int main()
{
    int row, col, sum = 0;
     
     printf("enter the no of rows and column:");
     scanf("%d",&n);
     
     int A[n][n]
  
    for(int i=0; i<n; i++)
    {
        for(int j=0; j<n; j++)
        {
            scanf("%d", &A[i][j]);
        }
    }

    for(i=0; i<n; i++)
    {
        sum = sum + A[i][i];
    }

    printf("\nSum of main diagonal elements = %d", sum);

    return 0;
}
